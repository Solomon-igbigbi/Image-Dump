<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/registerPageStyle/style.css">
    <title>Document</title>
</head>
<body>
    <a  class="nav-image" href="/"><img src="img/logo.png" alt="image-Dump"></a>
    <form action="/register" method="POST">
        <h1>CREATE AN ACCOUNT</h1>
        <div class="username">
            <label for="username">USERNAME</label>
            <input type="text" name="username" placeholder="Type in a username">
        </div>
        <div class="email">
            <label for="email">EMAIL</label>
            <input type="email" name="email" placeholder="Somebody@example.com">
        </div>
        <div class="password">
            <label for="password">PASSWORD</label>
            <input type="password" name="password" placeholder="Type in a password">
        </div>
        <button type="submit">SIGN UP</button>
    </form>
    <span>sign in with <a href="/">google</a></span><br>
    <span>already have an account? <a href="/login">sign in</a></span>
</body>
</html>